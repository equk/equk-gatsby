{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "es6"
        ],
        "module": "commonjs",
        "sourceMap": true,
        "jsx": "react",
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "declaration": true,
        "declarationMap": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "esModuleInterop": true,
    },
    "exclude": [
        "node_modules",
        "public",
        ".cache"
    ]
}